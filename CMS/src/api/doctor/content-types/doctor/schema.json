{
  "kind": "collectionType",
  "collectionName": "doctors",
  "info": {
    "singularName": "doctor",
    "pluralName": "doctors",
    "displayName": "Doctor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 20
    },
    "service": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service.service",
      "inversedBy": "doctors"
    },
    "description": {
      "type": "text",
      "maxLength": 550
    },
    "message": {
      "type": "text",
      "maxLength": 550
    },
    "doctor_information": {
      "type": "component",
      "component": "items.doctor-details",
      "repeatable": true
    },
    "doctor_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
