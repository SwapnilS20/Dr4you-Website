{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 30
    },
    "short_description": {
      "type": "string",
      "maxLength": 200
    },
    "icon": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "detailed_information": {
      "type": "blocks"
    },
    "doctors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::doctor.doctor",
      "mappedBy": "service"
    },
    "blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog",
      "mappedBy": "service"
    }
  }
}
