{
  "collectionName": "components_card_contact_cards",
  "info": {
    "displayName": "Contact-Card"
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 20
    },
    "details": {
      "type": "string",
      "maxLength": 50
    },
    "remixicon_classname": {
      "type": "string",
      "regex": "^ri-[a-z0-9-]+$"
    },
    "call_to_function": {
      "type": "enumeration",
      "enum": [
        "call",
        "whatsapp",
        "mail"
      ]
    }
  },
  "config": {}
}
